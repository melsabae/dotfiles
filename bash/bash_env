function readonly_export {
    readonly $1=$2
    export $1
}


readonly_export LESSHISTFILE /dev/null
readonly_export INPUTRC $HOME/.config/inputrc
readonly_export INDENT_PROFILE $HOME/.config/formatting/indent.pro
readonly_export ARTISTIC_STYLE_PROFILE $HOME/.config/formatting/astylerc
readonly_export EDITOR vim

